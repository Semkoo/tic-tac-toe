{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "yarn run prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "yarn run prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@expo-google-fonts/delius-unicase": "^0.2.2",
        "@expo-google-fonts/inter": "^0.2.2",
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/native-stack": "^6.2.5",
        "@react-navigation/stack": "^6.0.11",
        "expo": "~44.0.0",
        "expo-app-loading": "~1.3.0",
        "expo-font": "~10.0.4",
        "expo-linear-gradient": "~11.0.1",
        "expo-status-bar": "~1.2.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "0.64.3",
        "react-native-reanimated": "^2.3.1",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.10.1",
        "react-native-web": "0.17.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.64.12",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.28.0",
        "husky": "^4.3.0",
        "lint-staged": "^12.1.7",
        "prettier": "2.5.1",
        "typescript": "~4.3.5"
    },
    "private": true,
    "engines": {
        "node": "^16.13.1"
    }
}
